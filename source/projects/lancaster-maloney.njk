		<figure>
			<figcaption x-text="translation[$store.global.language]['imageCaption1']"></figcaption>
			<picture>
				<source srcset="./images/project/{{ project }}/1.webp" type="image/webp">
				<source srcset="./images/project/{{ project }}/1.png" type="image/png">
