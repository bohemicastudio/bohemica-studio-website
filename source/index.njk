{% extends "./includes/default.njk" %}

{% block title %}Bohemica Studio{% endblock %}

{% block head %}
	<script>

	</script>
{% endblock %}

{% block content %}
	<div class="overflow-hidden relative bg-blue-gray-100 select-none">

		{# <div class="block w-full min-h-screen">
				<span>Hello</span>
			</div> #}

		<div class="relative min-h-screen">

			<div class="flex absolute inset-0 z-30 flex-col">
				<div class="container flex flex-row justify-between items-center pt-4">
					<img class="w-auto h-full opacity-0" src="images/logomark.svg" onload="anime({ targets: this, duration: 200, delay: 600, opacity: 1, easing: 'linear'})">
					<div class="flex py-2.5 px-3.5 space-x-3.5 bg-white rounded-2.5xl border border-blue-gray-300 items-center top-menu">
						<a href="https://cal.mixmax.com/bohemicastudio/" class="py-2.5 px-3 text-sm font-medium leading-none text-warm-gray-800 bg-yellow-400 rounded-xl cursor-ne-resize" data-tippy-content="Schedule a call" target="_blank">Available for new projects</a>
						<button class="top-menu__button hover:bg-blue-200 focus:ring-blue-400" data-tippy-content="What we do">
							What
						</button>
						<button class="top-menu__button hover:bg-amber-200 focus:ring-amber-400" data-tippy-content="How we do it">
							How
						</button>
						<button class="top-menu__button hover:bg-red-200 focus:ring-red-400" data-tippy-content="How much it costs">
							How much
						</button>
						<button class="top-menu__button hover:bg-cool-gray-200 focus:ring-cool-gray-400" data-tippy-content="Why to work with us">
							Why
						</button>
						<button class="top-menu__button hover:bg-green-200 focus:ring-green-400" data-tippy-content="Who we have worked with">
							Who
						</button>
						<button class="top-menu__button hover:bg-teal-200 focus:ring-teal-400" data-tippy-content="Where to reach us">
							Where
						</button>
					</div>
				</div>

				<div class="container pt-6 h-7/12 hero">

					<div class="flex relative justify-center items-center w-full h-full">
						<div class="absolute left-0 opacity-0" style="top: 1rem" x-data="{ open: false }" x-on:click.away="open = false" x-init="() => { anime({ targets: $el, duration: 200, delay: 2800, opacity: 1, easing: 'linear'}) }">

							<div class="relative">

								<button x-on:click="open = !open" :class="{ 'zoom-0' : open }"
								        class="flex overflow-hidden relative z-50 py-1.5 pl-3.5 pr-1.5 text-gray-800 bg-blue-gray-700 rounded-xl group focus:outline-none focus:ring-2 ring-offset-2 ring-blue-gray-700 transition duration-100 ease-linear">

									<div class="flex flex-row justify-between items-center">

										<svg class="mr-2.5 text-blue-gray-100 flex-shrink-0 w-6 h-6" width="24" height="24" viewBox="0 0 24 24" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
											<path d="M12.87 15.07L10.33 12.56L10.36 12.53C12.1 10.59 13.34 8.36 14.07 6H17V4H10V2H8V4H1V5.99H12.17C11.5 7.92 10.44 9.75 9 11.35C8.07 10.32 7.3 9.19 6.69 8H4.69C5.42 9.63 6.42 11.17 7.67 12.56L2.58 17.58L4 19L9 14L12.11 17.11L12.87 15.07ZM18.5 10H16.5L12 22H14L15.12 19H19.87L21 22H23L18.5 10ZM15.88 17L17.5 12.67L19.12 17H15.88V17Z"></path>
										</svg>

										<span class="text-lg font-medium leading-none text-blue-gray-100" x-text="$store.global.languages.find(language => language.code === $store.global.language)?.name"></span>

										<span class="text-blue-gray-100 bg-blue-gray-700 rounded-lg ml-3.5 w-8 h-8 flex transition duration-100 ease-linear items-center justify-center group-hover:bg-blue-gray-500 group-focus:bg-blue-gray-500" data-tippy-content="Change language"><i class="gg-chevron-down"></i></span>
									</div>
								</button>

								<div class="absolute left-0 top-full z-10 p-1 mt-3 bg-blue-gray-500 rounded-lg origin-left" x-cloak
								     :class="open ? 'opacity-1' : 'opacity-0'"
								     :style="(open ? 'transform: scale(1) translateY(0rem); visibility: visible; transition: 200ms opacity linear 100ms, 200ms transform ease-out 100ms, 0ms visibility linear 0ms, 0ms overflow linear 0ms;' : 'transform: scale(0.85) translateY(-.75rem); visibility: hidden !important; transition: 200ms opacity linear 0ms, 200ms transform ease-out 0ms, 0ms visibility linear 200ms, 0ms overflow linear 150ms;') + ''">

									<div class="grid gap-2">
										<template x-for="language in $store.global.languages" :key="language.code">
											<button x-on:click="$store.global.switchTranslation(language.code); open = false"
											        class="flex flex-row items-center py-2 px-3 space-x-4 rounded-md transition duration-75 ease-linear cursor-pointer focus:bg-blue-gray-800 focus:outline-none hover:bg-blue-gray-700">
										<span class="w-7 h-auto">
											<img alt="language-icon" x-bind:src="'images/languages/' + language.code + '.svg'">
										</span>
												<span class="font-medium leading-none text-blue-gray-100 whitespace-nowrap" x-text="language.name"></span>
											</button>
										</template>
									</div>
								</div>

							</div>
						</div>

						<div class="flex flex-col justify-center items-center pt-2 space-y-6 w-8/12 text-center">
							<h2 class="tracking-tight leading-none text-blue-gray-700" style="font-size: 5rem" x-text="$store['translations.home'].return('head.title')"></h2>
							<span class="text-2xl text-3xl tracking-tight leading-relaxed text-blue-gray-700 font-dm-mono" x-text="$store['translations.home'].return('head.subtitle')"></span>
							<div class="flex flex-row items-center p-1 font-medium leading-none bg-teal-50 rounded-xl border-2 opacity-0 transition duration-150 ease-linear cursor-pointer border-teal2-400 group hover:border-teal2-500" x-data x-init="() => { anime({ targets: $el, duration: 200, delay: 2400, opacity: 1, easing: 'linear'}) }">
								<div class="flex items-center px-3 h-8 rounded-lg bg-teal2-200">
									<span class="text-teal2-800">Current activity:</span>
								</div>
								{# <span x-text="$store.activity.getUrl"></span>
								<span x-text="!$store.activity.getUrl?.includes('://')"></span> #}
								<a x-bind:href="$store.activity.getUrl" data-navigo="!$store.activity.getUrl?.includes('://')" target="_blank" class="flex items-center pr-1 pl-3 space-x-2 leading-none text-teal2-700 group-hover:text-teal2-800" x-html="$store.activity.getContent"></a>
								<i class="mr-1.5 transition duration-200 ease-out transform gg-chevron-right text-teal2-600 group-hover:text-teal2-800 group-hover:translate-x-0.5"></i>
							</div>
						</div>

						<div class="flex absolute right-0 bottom-0 flex-col space-y-1.5 opacity-0" x-data x-init="() => { anime({ targets: $el, duration: 200, delay: 2600, opacity: 1, easing: 'linear'}) }">

							<a href="https://twitter.com/BohemicaStudio" target="_blank" data-tippy-content="See what we have to say on Twitter" class="cursor-ne-resize p-3.5 text-blue-gray-500 rounded-full transition duration-100 ease-linear cursor-pointer hover:bg-blue-gray-300 hover:text-blue-gray-800">
								<div class="flex justify-center items-center w-8 h-8">
									<svg class="w-full h-full" xmlns="http://www.w3.org/2000/svg" width="28" height="23" fill="currentColor" viewBox="0 0 28 23">
										<path d="M28 2.694c-1.03.457-2.136.765-3.299.904A5.76 5.76 0 0027.227.42a11.468 11.468 0 01-3.648 1.394 5.745 5.745 0 00-9.937 3.934c0 .448.052.885.148 1.307A16.313 16.313 0 011.95 1.05a5.742 5.742 0 00-.777 2.888A5.745 5.745 0 003.728 8.72 5.731 5.731 0 011.126 8v.075a5.747 5.747 0 004.608 5.633 5.787 5.787 0 01-2.594.099 5.748 5.748 0 005.366 3.988 11.527 11.527 0 01-7.135 2.46A11.7 11.7 0 010 20.176a16.246 16.246 0 008.805 2.58c10.568 0 16.345-8.752 16.345-16.344 0-.246-.006-.495-.017-.742a11.677 11.677 0 002.864-2.972L28 2.694z"/>
									</svg>
								</div>
							</a>

							<a href="https://linkedin.com/company/bohemicastudio" target="_blank" data-tippy-content="Connect with us on LinkedIn" class="cursor-ne-resize p-3.5 text-blue-gray-500 rounded-full transition duration-100 ease-linear cursor-pointer hover:bg-blue-gray-300 hover:text-blue-gray-700">
								<div class="flex justify-center items-center w-8 h-8">
									<svg class="w-full h-full" xmlns="http://www.w3.org/2000/svg" width="28" height="28" fill="currentColor" viewBox="0 0 28 28">
										<path d="M28 2.059V25.94A2.059 2.059 0 0125.941 28H2.06A2.059 2.059 0 010 25.941V2.06A2.059 2.059 0 012.059 0H25.94A2.059 2.059 0 0128 2.059zM8.235 10.706H4.118v13.176h4.117V10.706zm.37-4.53a2.372 2.372 0 00-2.354-2.388h-.075a2.388 2.388 0 100 4.777 2.372 2.372 0 002.43-2.314v-.075zm15.277 9.702c0-3.962-2.52-5.502-5.023-5.502a4.694 4.694 0 00-4.167 2.125h-.116v-1.795h-3.87v13.176h4.117v-7.008a2.734 2.734 0 012.471-2.948h.157c1.309 0 2.28.823 2.28 2.899v7.057h4.118l.033-8.004z"/>
									</svg>
								</div>
							</a>
						</div>

					</div>
				</div>
			</div>

			<div class="container absolute inset-0 z-10">
				<img class="absolute bottom-0 left-0 origin-bottom-left" style="height: 80vh; transform: translateY(32.5vh); opacity: 0" id="blueHand" src="images/blue-hand.png" onload="
				anime({ targets: this, duration: 400, delay: 1600, opacity: 1, easing: 'linear'})
				anime({ targets: this, duration: 2600, delay: 1600, rotate: [-4, 12], easing: 'easeOutQuart' })">
				<img class="absolute bottom-0 origin-bottom-right" style="left: 40%; height: 62.5vh; transform: translateY(27.5vh); opacity: 0" id="greenHand" src="images/green-hand.png" onload="
				anime({ targets: this, duration: 400, delay: 400, opacity: 1, easing: 'linear', })
				anime({ targets: this, duration: 1200, delay: 400, rotate: [4, -12], easing: 'easeOutQuart', })">
				<img class="absolute bottom-0 origin-bottom-left" style="left: 50%; height: 57.5vh; transform: translateY(27.5vh); opacity: 0" id="purpleHand" src="images/purple-hand.png" onload="
				anime({ targets: this, duration: 400, delay: 1000, opacity: 1, easing: 'linear', })
				anime({ targets: this, duration: 1400, delay: 1000, rotate: [6, 18], easing: 'easeOutQuart', })">
				<img class="absolute right-0 bottom-0 origin-bottom-left" style="height: 70vh; transform: translateY(22.5vh); opacity: 0" id="yellowHand" src="images/yellow-hand.png" onload="
				anime({ targets: this, duration: 400, delay: 2000, opacity: 1, easing: 'linear', })
				anime({ targets: this, duration: 2400, delay: 2000, rotate: [8, -4], easing: 'easeOutQuart', })">
			</div>

			{# Warp background #}
			<div class="absolute inset-0">
				<img class="object-cover top-0 left-0 z-0 h-full opacity-0" src="images/warp-background.svg" onload="anime({targets: this, duration: 100, delay: 100, opacity: 1, easing: 'linear'})">
			</div>
		</div>

		<div class="container">

			{# Company name title #}
			<div class="flex relative z-30 justify-center">
				<svg class="text-blue-400 -mb-1.5 w-full" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 1240 128">
