							<img id="preview-image-{{ project }}" class="mx-auto w-auto" data-zoom src="/images/projects/{{ project }}/{{ image }}">
						</div>
					</div>
					<div class="flex justify-center items-center py-4 bg-cool-gray-900 rounded-b-3xl border-r border-b border-l border-cool-gray-500">
						<span class="w-24 h-1 bg-cool-gray-600 rounded-full"></span>
					</div>
				</div>

