			<div class="flex flex-col items-start space-y-10">
				<div class="grid gap-y-3 gap-x-5 font-medium whitespace-nowrap" style="grid-template-columns: min-content auto">
					<span class="text-cool-gray-400 font-dm-mono" x-text="$store['translation.projectExamples'].return('labels.client') + ':'"></span>
					<span class="text-cool-gray-100" x-text="$store['translation.projectExamples'].return(`projects['{{ project }}'].client`)"></span>
					<span class="text-cool-gray-400 font-dm-mono" x-text="$store['translation.projectExamples'].return('labels.year') + ':'"></span>
					<span class="text-cool-gray-100" x-text="$store['translation.projectExamples'].return(`projects['{{ project }}'].year`)"></span>
					<span class="text-cool-gray-400 font-dm-mono" x-text="$store['translation.projectExamples'].return('labels.type') + ':'"></span>
					<span class="text-cool-gray-100" x-text="$store['translation.projectExamples'].return(`projects['{{ project }}'].type`)"></span>
					<span class="text-cool-gray-400 font-dm-mono" x-text="$store['translation.projectExamples'].return('labels.stack') + ':'"></span>
					<span class="text-cool-gray-100" x-text="$store['translation.projectExamples'].return(`projects['{{ project }}'].stack`)"></span>
				</div>

				<a {# x-bind:href="window.router.getPath('overview')" #} href="javascript:;" class="flex flex-row items-center py-2 px-3.5 space-x-1 font-medium bg-emerald-400 hover:bg-emerald-200 rounded-lg ring-offset-2 transition duration-100 ease-linear focus:ring-2 group focus:ring-offset-gray-legacy-800 focus:ring-emerald-200" data-navigo>
					<span class="leading-none text-cool-gray-800 whitespace-nowrap group-hover:text-cool-gray-900">See the project</span>
					<i class="transition duration-200 ease-out transform gg-chevron-right text-cool-gray-800 group-hover:text-cool-gray-900 group-hover:translate-x-0.5 flex-shrink-0"></i>
				</a>
			</div>

		</div>
	</div>

{% endmacro %}

